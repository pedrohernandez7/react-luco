    "@stackblitz/sdk": "^1.9.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.8",
    "isbot": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@netlify/remix-adapter": "^2.6.1",
    "@remix-run/dev": "*",
    "@tailwindcss/typography": "^0.5.10",
