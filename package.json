{
  "name": "react-component-anatomy",
  "version": "1.0.0",
  "description": "A Remix application demonstrating the anatomy of a React component.",
  "keywords": [
    "react",
    "remix",
    "component",
    "anatomy",
    "example"
  ],
  "author": "ptomas@iriusrisk.com",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
